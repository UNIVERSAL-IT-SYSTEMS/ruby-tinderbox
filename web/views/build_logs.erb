<div class="text-center">
	<h1><%=build[:package_id]%></h1>
	<h3><%=Time.at(build[:time].to_i).strftime('%Y-%m-%d')%></h3>
	<div class="btn-group btn-group-justified">
		<a class="btn btn-default <%="disabled" if build[:emerge_info].nil?%>" href="#emerge_info">Emerge Info</a>
		<a class="btn btn-default <%="disabled" if build[:emerge_pqv].nil?%>" href="#emerge_pqv">Emerge PQV</a>
		<a class="btn btn-default <%="disabled" if build[:build_log].nil?%>" href="#build_log">Build Log</a>
		<a class="btn btn-default <%="disabled" if build[:gem_list].nil?%>" href="#gem_list">Gem List</a>
	</div>
</div>
<br>

<%unless build[:emerge_info].nil?%>
<div class="panel panel-default">
	<div class="panel-heading">
		<h4><a name="emerge_info">Emerge Info</a></h4>
	</div>
	<div class="panel-body">
		<pre><%=build[:emerge_info]%></pre>
	</div>
</div>
<%end%>

<%unless build[:emerge_pqv].nil?%>
<div class="panel panel-default">
	<div class="panel-heading">
		<h4><a name="emerge_pqv">Emerge PQV</a></h4>
	</div>
	<div class="panel-body">
		<pre><%=build[:emerge_pqv]%></pre>
	</div>
</div>
<%end%>

<%unless build[:build_log].nil?%>
<div class="panel panel-default">
	<div class="panel-heading">
		<h4><a name="build_log">Build Log</a></h4>
	</div>
	<div class="panel-body">
		<pre><%=build[:build_log]%></pre>
	</div>
</div>
<%end%>

<%unless build[:gem_list].nil?%>
<div class="panel panel-default">
	<div class="panel-heading">
		<h4><a name="gem_list">Gem List</a></h4>
	</div>
	<div class="panel-body">
		<pre><%=build[:gem_list]%></pre>
	</div>
</div>
<%end%>
